function integer main[]

	string Nombre<-"Dave Mustaine";
	integer Edad<-"53";
	character Sexo<-\M\;
	number Estatura<-1.83;
	boolean Vivo<-true;  


	if(not Vivo)
		print("Esta muerto");
	end

	if(Nombre = "Dave Mustaine" and (Edad > 50))

		for(integer i<-0; i < Edad; i<-i+1)  
			     print("Valor de i: " + i);
			for(integer j<-i; j = 0 ; j--)
				print("Valor de j: " + j);
			end
		end

	elseif(Sexo <> \M\ or (Estatura >= 1.80)) 
		
		if(Sexo = \F\)
			print("Es alta la mujer");
		end

	elseif(Nombre <> "Dave Mustaine")
		
		Impresion["No es Dave"];

	else
	
		Impresion["Es un alien" + "?"];
	end	

	give_back 0;

	end


function void Impresion [string text]
	print(text);
end