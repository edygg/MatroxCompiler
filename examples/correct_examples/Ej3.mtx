function integer main[]

	integer contador<-1;
	integer num<-0; 
	integer factorial<-0;

	print("Ingrese su numero: ");
	get_value num;

	while(contador <= num)
		factorial*<-contador;
		contador++;
	end

	print("El Factorial del numero: ");
	print(num); 
	print(" es: ");
	print(factorial);

	give_back 0;
	
end